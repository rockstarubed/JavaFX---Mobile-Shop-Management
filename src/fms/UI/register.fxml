<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" focusTraversable="true" prefHeight="320.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fms.RegisterController">
   <children>
      <Label layoutX="129.0" layoutY="71.0" prefHeight="16.0" prefWidth="65.0" text="Name" />
      <Label layoutX="129.0" layoutY="143.0" prefHeight="16.0" prefWidth="65.0" text="UserID" />
      <Label layoutX="129.0" layoutY="214.0" prefHeight="16.0" prefWidth="65.0" text="Password" />
      <TextField fx:id="username" layoutX="242.0" layoutY="66.0" onAction="#addaccount" prefHeight="26.0" prefWidth="250.0" promptText="First &amp; Last Name" />
      <TextField fx:id="userid" layoutX="242.0" layoutY="138.0" onAction="#addaccount" prefHeight="26.0" prefWidth="250.0" promptText="Create New userID" />
      <PasswordField fx:id="passwd" layoutX="242.0" layoutY="209.0" onAction="#addaccount" prefHeight="26.0" prefWidth="250.0" promptText="Create New Password" />
      <Button layoutX="165.0" layoutY="263.0" mnemonicParsing="false" onAction="#addaccount" text="Submit" />
      <Button layoutX="261.0" layoutY="263.0" mnemonicParsing="false" onAction="#reset" prefHeight="26.0" prefWidth="65.0" text="Reset" />
      <Button fx:id="ex" cancelButton="true" layoutX="367.0" layoutY="263.0" mnemonicParsing="false" onAction="#exit" prefHeight="26.0" prefWidth="65.0" text="Exit" />
      <Label fx:id="status" layoutX="227.0" layoutY="14.0" text="Create A New Account" />
   </children>
</AnchorPane>
